{
  "name": "seuspos",
  "private": true,
  "version": "0.0.0",
  "description": "SeusPOS - Sistema de punto de venta y contabilidad.",
  "author": "SeusPOS",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:electron": "powershell -NoProfile -ExecutionPolicy Bypass -Command \"$env:VITE_DEV_SERVER_URL='http://localhost:5173'; npx electron .\"",
    "dev:app": "powershell -NoProfile -ExecutionPolicy Bypass -Command \"Start-Process powershell -ArgumentList ' -NoProfile -ExecutionPolicy Bypass -Command npm run dev'; Start-Sleep -Seconds 2; $env:VITE_DEV_SERVER_URL='http://localhost:5173'; npx electron .\"",
    "build": "vue-tsc && vite build && electron-builder",
    "preview": "vite preview"
  },
  "dependencies": {
    "jspdf": "^4.0.0",
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.4",
    "electron": "^30.0.1",
    "electron-builder": "^24.13.3",
    "typescript": "^5.2.2",
    "vite": "^5.1.6",
    "vite-plugin-electron": "^0.28.6",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vue-tsc": "^2.0.26"
  },
  "main": "dist-electron/main.js"
  ,
  "build": {
    "directories": {
      "output": "release",
      "buildResources": "build"
    },
    "appId": "com.seuspos.app",
    "productName": "SeusPOS",
    "executableName": "SeusPOS",
    "win": {
      "icon": "seuspos.png",
      "artifactName": "SeusPOS-Setup-${version}.${ext}",
      "signAndEditExecutable": false
    }
  }
}
